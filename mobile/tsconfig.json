{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ES2021", "DOM"],
    "target": "ES2021",
    "moduleResolution": "node",
    "types": ["react", "react-native", "expo"],
    "skipLibCheck": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": "."
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
