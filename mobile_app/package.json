{
  "name": "ai-sdlc-assistant-mobile",
  "version": "2.0.0",
  "description": "Advanced AI SDLC Assistant Mobile App with offline capabilities and cross-platform sync",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace AiSdlcAssistant.xcworkspace -scheme AiSdlcAssistant -configuration Release -destination generic/platform=iOS -archivePath AiSdlcAssistant.xcarchive archive",
    "metro": "metro start",
    "postinstall": "npx react-native setup-ios-permissions"
  },
  "dependencies": {
